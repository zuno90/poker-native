    "expo": {
        "name": "app",
        "slug": "app",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./assets/logo.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
